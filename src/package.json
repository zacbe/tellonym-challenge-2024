{
  "name": "tellonym-api-chat",
  "version": "1.0.1",
  "description": "chat api for tellonym-api",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:prod": "rm -rf dist && tsc --p tsconfig.production.json",
    "build:docker": "docker build --build-arg NODE_VERSION=$(cat .nvmrc | tr -cd [:digit:].) --tag tellonym-chat-api:latest .",
    "cm": "git-cz",
    "deploy": "npm run-script build:prod && eb deploy chat-api-production",
    "deploy-testing": "npm run-script build:prod && eb deploy chat-api-test",
    "deploy-staging": "npm run-script build:prod && eb deploy chat-api-staging",
    "eslint": "eslint --report-unused-disable-directives ./src/",
    "eslint:fix": "eslint --fix ./src/",
    "eslint:report": "yarn eslint --ext .js,.jsx,.ts,.tsx --output-file=eslint-report.json --format=json",
    "prod": "npm run-script build && node ./dist/server.js",
    "release": "np --no-cleanup",
    "start": "node dist/server.js",
    "test": "NODE_ENV=test jest --coverage=false --runInBand --forceExit --watch",
    "test:watch": "npm run-script test --watch",
    "test:coverage": "NODE_ENV=test jest --runInBand --forceExit",
    "test:ci": "NODE_ENV=test jest --coverage=false --runInBand --forceExit --verbose",
    "version": "conventional-changelog -p @tellonym/tellonym -o CHANGELOG.md -r 50 && git add CHANGELOG.md",
    "watch": "ts-node-dev --no-notify --respawn --transpileOnly ./src/server.ts"
  },
  "author": "Max Fehmerling <fehmer@tellonym.me>",
  "license": "UNLICENSED",
  "dependencies": {
    "@sentry/node": "^5.27.2",
    "@tellonym/enums": "^14.2.1",
    "@tellonym/rlt-stats-client": "^4.0.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.19.0",
    "clickhouse": "^2.2.1",
    "debug": "^4.3.1",
    "dotenv": "^6.0.0",
    "eol": "^0.9.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-async-handler": "^1.2.0",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "qs": "^6.9.4",
    "ratelimit.js": "^1.8.0",
    "redis": "^3.0.2",
    "tellonym-models": "^14.17.1",
    "uWebSockets.js": "git+https://github.com/uNetworking/uWebSockets.js#v18.8.0",
    "useragent": "^2.3.0",
    "uuid": "^3.4.0",
    "validator": "^11.1.0",
    "winston": "^2.4.4",
    "zookeeper": "4.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@tellonym/conventional-changelog-tellonym": "7",
    "@tellonym/eslint-config-typescript": "^3.0.0",
    "@types/bluebird": "^3.5.24",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "4.17.0",
    "@types/express-jwt": "0.0.42",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lusca": "^1.5.0",
    "@types/node": "^12.12.38",
    "@types/qs": "^6.5.3",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.6",
    "@types/useragent": "^2.1.1",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.11.1",
    "@types/winston": "^2.3.7",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-tellonym": "^5.2.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^23.10.0",
    "git-cz": "^4.3.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "np": "^6.2.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.11.0",
    "typescript": "4.3"
  },
  "engines": {
    "node": ">=12 <13",
    "npm": "PLEASE USE YARN"
  }
}
